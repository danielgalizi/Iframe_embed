<script>
 var URLparam = location.search.split('url=')[1]
 document.write('<div id="iframe"><iframe id="NewIFrame" style="position:fixed; top:0px; left:0px; bottom:0px; width:100%; height:100%; border:none; margin:0; padding:0; overflow:hidden;" src='+URLparam+'><\/iframe></div>');
</script>
</body>
